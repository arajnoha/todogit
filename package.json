{
  "name": "todogit",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-builder --mac --x64"
  },
  "build": {
    "appId": "com.arajnoha.todogit",
    "productName": "TodoGit",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "icon": "app.png"
    }
  },
  "dependencies": {
    "axios": "^0.25.0"
  },
  "devDependencies": {
    "electron-builder": "^24.13.3",
    "electron": "^16.2.8"
  }
}